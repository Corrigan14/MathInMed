                    {% image '@AppBundle/Resources/public/images/general/wallpaper-simple-2.jpg' %}
                    <img src="{{ asset_url }}" alt="home_slider2" data-bgposition="center top" data-bgfit="cover"
                         data-bgrepeat="no-repeat">
                    {% endimage %}

                    <!-- LAYER NR. 1 -->
                    <div class="tp-caption sfb tp-resizeme"
                         data-x="right" data-hoffset="0"
                         data-y="center" data-voffset="90"
                         data-speed="1000"
                         data-start="2000"
                         data-easing="Power3.easeInOut"
                         data-splitin="chars"
                         data-splitout="none"
                         data-elementdelay="0.06"
                         data-endelementdelay="0.1"
                         data-endspeed="300"
                         style="z-index: 5; font-size: 30px; color:white; max-width: auto; max-height: auto; white-space: nowrap;">
                        {{ 'slider.default.joke-1-1'|trans }}
