<!-- Header -->
<header class="header single-pages">
    <div class="sticky dark-header">
        <div class="container">
            <div class="logo">
                <a href="{{ path('app_homepage') }}">
                    {% image '@AppBundle/Resources/public/templateEvanto/images/logo.png' %}
                    <img src="{{ asset_url }}" alt="logo">
                    {% endimage %}
                </a>
            </div>

            <!-- Vertical Navbar -->
            <nav>
                <ul id="ownmenu" class="ownmenu">
                    <li class="scroll active">
                        <a href="{{ path('app_homepage') }}">
                            {{ 'main_navbar.about'|trans }}
                        </a>
                    </li>
                    <li class="scroll">
                        <a href="{{ path('app_book_main_introudction') }}">
                            {{ 'main_navbar.book'|trans }}
                        </a>
                    </li>
                    <li class="scroll"><a href="#footer">{{ 'main_navbar.contact'|trans }}</a></li>
                </ul>
            </nav>
        </div>
    </div>
</header>
<!-- End Header -->
