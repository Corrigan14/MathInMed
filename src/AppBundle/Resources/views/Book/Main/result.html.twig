                    <!-- Icon Row -->
                    <ul class="row">

                        <!-- Services -->
                        <li class="col-md-4 animate fadeInUp" data-wow-delay="0.4s"> <img class="img-responsive" src="images/services/img-1.jpg" alt="" > <i class="fa fa-laptop margin-top-20"></i>
